<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
    <item name="android:actionBarStyle">@style/MyActionBar</item>
    </style>



    <style name="Profile" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="test" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="MyCustomDialog" parent="Base.Theme.AppCompat.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>


    <style name="AppBaseTheme" parent="Base.Theme.AppCompat.Light">
        <item name="actionBarStyle">@style/MyActionBar</item>
    </style>

    <!-- ActionBar styles -->
    <style name="MyActionBar" parent="Widget.AppCompat.Light.ActionBar.Solid.Inverse">

        <item name="background">@drawable/tab_3</item>
    </style>

    <style name="editText">
        <item name="android:background">@drawable/tab_1</item>
        <item name="android:textColorHint">#b5151617</item>


    </style>


</resources>
